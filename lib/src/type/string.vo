
use mem;

class String {
    data: ptr
    bytes: uint

    static fn make_from_ptr(data: ptr, length: uint) String {
        let d = mem:alloc(length + 1)
        mem:copy(data, d, length)
        @ptrv(d, u8, length) = 0
        return String {
            data: d
            bytes: length
        }
    }
}
